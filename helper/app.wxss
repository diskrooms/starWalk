/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

/* 公共样式*/
.block{display: block;}
.px-8{font-size: 8rpx;}
.px-10{font-size: 10rpx;}
.px-12{font-size: 12rpx;}
.px-14{font-size: 14rpx;}
.px-16{font-size: 16rpx;}
.px-18{font-size: 18rpx;}
.px-20{font-size: 20rpx;}
.px-22{font-size: 22rpx;}
.px-24{font-size: 24rpx;}
.px-26{font-size: 26rpx;}
.px-28{font-size: 28rpx;}
.px-30{font-size: 30rpx;}
.px-32{font-size: 32rpx;}
.left{float:left}
.right{float:right}
.inline{display: inline;}
.block{display:block}
.flex{display: flex}
.inline-block{display:inline-block}
.show{display: auto;}
.hide{display: none;}
.clearfix:after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
.text-align-center{text-align: center}
.text-align-left{text-align: left;}
.bg-blue2{background: #1291d4;}
.bg-blue3{background: #1f8cc6;}

page{background-color: #f5f5f5;}